{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "dart: run build_runner build",
			"type": "flutter",
			"command": "dart",
			"args": [
				"run",
				"build_runner",
				"build",
				"--delete-conflicting-outputs"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
		},
		{
			"label": "dart: run custom linit",
			"type": "flutter",
			"command": "dart",
			"args": [
				"run",
				"custom_lint"
			],
			"problemMatcher": [
				"$dart-build_runner"
			]
		},
	]
}