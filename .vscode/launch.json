{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "UKHSC Mobile (debug mode)",
            "request": "launch",
            "type": "dart",
            "preLaunchTask": "${defaultBuildTask}",
        },
        {
            "name": "UKHSC Mobile (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "preLaunchTask": "${defaultBuildTask}",
        },
        {
            "name": "UKHSC Mobile (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "preLaunchTask": "${defaultBuildTask}",
        },
        {
            "name": "UKHSC Mobile (web)",
            "type": "dart",
            "request": "launch",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "3000",
                // "--wasm",
                "--dart-define=GOOGLE_OAUTH_CLIENT_ID=1025435960200-t4cj1deguaaeocs0rtp03l9ki4spd58q.apps.googleusercontent.com",
            ],
            "preLaunchTask": "${defaultBuildTask}",
            "osx": {
                "env": {
                    "CHROME_EXECUTABLE": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome"
                },
            }
        }
    ]
}